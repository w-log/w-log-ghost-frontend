{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier",
        "next/core-web-vitals"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "plugins": ["react", "@typescript-eslint", "prettier", "unused-imports"],
    "rules": {
        "no-unused-vars": "warn",
        "unused-imports/no-unused-imports": "error"
    }
}
